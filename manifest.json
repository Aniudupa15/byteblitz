{
    "manifest_version": 3,
    "name": "Dark Pattern Checker",
    "version": "1.0",
    "description": "Highlight dark patterns on websites",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  